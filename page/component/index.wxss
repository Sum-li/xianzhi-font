}


.search-box{
    /* position: fixed; */
    
}
.search {
    position: relative;
    /* top:30rpx; */
    /* left:30rpx; */
    background-color:rgba(237,237,237,0.8);
    border-radius:4rpx;
    z-index:4;
    padding:15rpx;
    width:88%;
    margin:0 auto;
    /* overflow: auto; */

}
.search input{
}
.search button{
    display: inline-block;
    position: absolute;
    right:0rpx;
    top:0rpx;
    border: 0;
    background-color: transparent;
}
    
.dec{
    position: absolute;
    width: 100%;
    height: 170rpx;
    /* border-radius: 0 0 20rpx 20rpx; */
    z-index: -1;
    background-color: #AB956D;
}
.swiper-box{
    /* background-color: #AB956D; */
    position: relative;
    margin-top: 20rpx;
    background-position-y: -100rpx;
}

.select-title,.newgood-title{
    width: 100%;
    padding:30rpx 0;
    height: 40rpx;
    color: #AB956D;
    text-align: center;
}

.rank-box{
    /* height: 550rpx; */
    white-space: nowrap;
}
.rank-item{
    width: 300rpx;
    margin: 20rpx;
    border-radius: 10rpx;
    overflow: hidden;
    box-shadow: 0 0 10rpx 5rpx rgba(0,0,0,0.1);
    display: inline-block
}
.good-gphoto{
    height: 420rpx;
    width: 100%;
}
.selected .good-name1{
    width: 100%;
    /* display:-webkit-box; */
    -webkit-box-orient:vertical;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-line-clamp:2;
    font-family:'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
    font-size:10pt;
    padding:15rpx;
    color:gray; 
}






  /* 下拉菜单 */
  /* 下拉菜单导航 */
.orderBy{
    /* margin-bottom: 50rpx; */
}
  .menu-po {
    position: relative;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}
.icon-right{
    transition: all 0.3s;
    margin-left: 5rpx;
    color:#696969;
    transform: translateY(-5rpx)
}
.icon-down{
    transform: rotate(90deg);
    /* transform:translateX(5rpx) */
}
.menu {
    background-color: #fff;
    margin-top: 30rpx;
    /* border-top: 1rpx solid #e9e9e9; */
}

.menu-list {
    display: flex;
    /* justify-content: space-between; */
    /* height: 75rpx; */
    padding: 20rpx;
}

.menu-nav {
    /* background-color: #ededed; */
    /* width: 20%; */
    margin-right: 20rpx;
    border-radius: 4rpx;
    text-align: center; 
    padding: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.menu-nav icon{
    position: relative;
    
}
.menu-nav-on{
    background-color: #ededed;
}

.menu-nav image {
    width: 32rpx;
    height: 32rpx;
}

.menu-nav text {
    color: #696969;
    font-size: 14px;
}



.menu-if {
    background-color: #ffffff;
    box-sizing: border-box;
    position: absolute;
    top: 92rpx;
    left: 0;
    z-index: 9;
    width: 100%;
}

.meun-txt {
    height: auto;
    color: #696969;
}

.menu-text-list {
    position: relative;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

.menu-text-list-left {
    width: 100%;
    background-color: #fff;
    box-sizing: border-box;
    padding-left: 35rpx;
}

/* text样式 */
.meun-city-txt {
    display: block;
    border-bottom: 1rpx solid #e9e9e9;
    width: 100%;
    padding: 25rpx 0;
    font-size: 14px;
}

.meun-city-txt-price {
    display: flex;
    justify-content: center;
}

.menu-text-list-rig {
    width: 50%;
    position: absolute;
    top: 0;
    right: 0;
    background-color: #f9f9f9;
    z-index: 10;
    box-sizing: border-box;
    padding-left: 35rpx;
}


/* over/auto */
.over-auto {
    height: 550rpx;
    overflow: auto;
}













.newgood{
    padding: 0 10rpx;
    background-color: white;
}

.newgood-list {
    display: block;
    margin:0 0rpx 20rpx 0;
    border-radius: 4px;
    overflow: hidden;
    padding-bottom: 25rpx;
    border-bottom: 2rpx solid #ededed
}

.nav image{
    width:250rpx;
    height:250rpx;
    border-radius: 4rpx;
}
.newgood-text{
    font-size: 30rpx;
    text-align: left;
}
.newgood-text-name{
    width: 120rpx;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-line-clamp: 1;    /* white-space:nowrap;   */
}
.nav{
    overflow: hidden;

}
.nav>view{
    float: left;    
}
.nav-left{
    height: 250rpx;
}

.nav-right{
    position: relative;
    height:220rpx;
    width: 63%;
    padding: 15rpx 0 15rpx 15rpx;
}
.good-name{      /*只显示两行+超出部分省略号*/
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-line-clamp: 2;    /* white-space:nowrap;   */
}
.good-peice{
    color:rosybrown;
    font-size: 40rpx;
    margin-top: 15rpx;
}
.good-hot{
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
    font-size: 20rpx;
    color: rosybrown;
    /* display: flex; */
}
.newgood-text-user{
    width: 100%;
    position: absolute;
    bottom: 0rpx;
    display: flex;
    overflow: hidden;
    align-items: center;
    font-size: 24rpx;
    color: gray
}
.newgood-text-user icon{
    /* font-size: 16px; */
    position: relative;
    top:-6rpx;
}
.newgood-text-user view{
    margin-right: 0rpx;
    margin-left: 10rpx;

}
.newgood-text-school{
    color: rosybrown;
    position: absolute;
    bottom: 40rpx;
    left: 10rpx;
}
.bottom{
    height: 60rpx;
    /* background-color: #f5f5f5; */
    color:gray;
    text-align: center;
}
